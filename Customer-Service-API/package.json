{
  "name": "final-project-api-service",
  "version": "1.0.0",
  "description": "final-project-api-service",
  "author": "ducvd",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "killall -9 node && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./data-source.ts",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./data-source.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^3.7.0",
    "@liaoliaots/nestjs-redis": "^9.0.1",
    "@nestjs/common": "^9.0.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^9.0.5",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@opentelemetry/auto-instrumentations-node": "^0.31.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.31.0",
    "@opentelemetry/propagator-b3": "^1.5.0",
    "@opentelemetry/resources": "^1.5.0",
    "@opentelemetry/sdk-node": "^0.31.0",
    "aws-sdk": "^2.1390.0",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.0.0",
    "elastic-apm-node": "^3.37.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^5.1.1",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nest-winston": "^1.7.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.10.0",
    "project-version": "^2.0.0",
    "qs": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.30.0",
    "sequelize-typescript": "^2.1.5",
    "typeorm": "^0.3.14",
    "winston": "^3.8.1",
    "winston-elasticsearch": "^0.17.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.5",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^18.6.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.31.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.73.0",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
